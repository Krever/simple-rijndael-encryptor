      <ProgressIndicator fx:id="progressIndicator" visible="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="21.0" prefWidth="26.0" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.rowIndex="4" />
